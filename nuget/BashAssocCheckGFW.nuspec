<?xml version="1.0"?>
<package >
  <metadata>
    <id>BashAssocFixForGitWin</id>
    <version>0.0.1</version>
    <authors>Jake Stine</authors>
    <owners>Jake Stine</owners>
    <projectUrl>https://github.com/jstine35/BashPipeFixGFW</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Fixes a bug in Git for Windows .sh filetype and allows embedding .sh scripts into batch files and build events.</description>
    <releaseNotes>Initial package release.</releaseNotes>
    <copyright>Copyright 2018</copyright>
    <tags>Git SH Bash Assoc</tags>

    <!--
	<dependencies>
      <dependency id="SampleDependency" version="1.0" />
    </dependencies>
	-->
  </metadata>
  <files>
    <file src="$eudodir$eudo.exe"                   target="bin" />
    <file src="..\sh_auto_file_fix.sh"              target="bin" />
    <file src="..\sh_assoc_check.sh"                target="bin" />
    <file src="sh-assoc-fix.cmd"                    target="bin" />
    <file src="BashAssocCheckGFW.targets"           target="build" />
  </files>
</package>